resource_name = "tex_medal"
script/source = "extends TextureRect

const spr_medal_bronze = preload(\"res://sprites/medal_bronze.png\")
const spr_medal_silver = preload(\"res://sprites/medal_silver.png\")
const spr_medal_gold = preload(\"res://sprites/medal_gold.png\")
const spr_medal_platinum = preload(\"res://sprites/medal_platinum.png\")

signal shown

func _ready():
	hide()
	var hbox_score_last = utils._get_main_node().find_node(\"hbox_score_last\")
	if hbox_score_last:
		hbox_score_last.connect(\"counting_finished\", self, \"show_medal\")
	pass
	
func show_medal():
	var texture = null
	if balance_game.balance_score_current >= balance_game.MEDAL_BRONZE:
		texture = spr_medal_bronze
	if balance_game.balance_score_current >= balance_game.MEDAL_SILVER:
		texture = spr_medal_silver
	if balance_game.balance_score_current >= balance_game.MEDAL_GOLD:
		texture = spr_medal_gold
	if balance_game.balance_score_current >= balance_game.MEDAL_PLATINUM:
		texture = spr_medal_platinum
	
	if texture != null:
		set_texture(texture)
		show()
		emit_signal(\"shown\")
	pass"
